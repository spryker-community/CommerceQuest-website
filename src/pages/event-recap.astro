---

<MainLayout
  title={pageTitle}
  structuredData={{
    "@context": "https://schema.org",
    "@type": "WebPage",
    "@id": "https://commercequest.space/",
    "url": "https://commercequest.space/",
    "name": "Hack, Build, Win. | CommerceQuest",
    "description": "Commerce Resources for the driven Spryker, Propel & Oryx developer.",
    "isPartOf": {
        "@type": "WebSite",
        "url": "https://commercequest.space/",
        "name": "CommerceQuest",
        "description": "Commerce Resources for the driven Spryker, Propel & Oryx developer."
    },
    "inLanguage": "en-US"
  }}
>
  <!--MainSection is the introductory section of the page, it also contains a CTA button-->
  <MainSection
    title="CommerceQuest Event Recaps"
    description="We and our community partners regularly organize events (like hackathons and meetups) to bring together software developers, engineers and architects. These events are a great opportunity to learn, network, and co-create with like-minded individuals. Check out the event recaps below to learn more about our past events or click the button below to view upcoming events."
    btnExists={true}
    btnTitle="Upcoming Events"
    btnURL="https://forum.commercequest.space/events/category"
  />
  {
    events.map((event, index) => {
      const imagePosition = index % 2 === 0 ? 'left' : 'right';

      return (
        <Recap
          title={event.title}
          description={event.description}
          imagePosition={imagePosition}
          imgOne={event.imgOne}
          imgOneAlt={event.imgOneAlt}
          imgTwo={event.imgTwo}
          imgTwoAlt={event.imgTwoAlt}
          btnExists={event.btnExists}
          btnTitle={event.btnTitle}
          btnURL={event.btnURL}
          location={event.location}
          organizer={event.organizer}
          github={event.github}
          logo={event.logo}
          logoAlt={event.logoAlt ?? ""}
          githubExists={event.githubExists}
          youtubeExists={event.youtubeExists}
          youtube={event.youtube}
          photoExists={event.photoExists}
          photoURL={event.photoURL}
        />
      );
    })
  }
</MainLayout>
