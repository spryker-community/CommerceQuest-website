{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "strictNullChecks": true,
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@content/*": [
        "src/content/*"
      ],
      "@data/*": [
        "src/data_files/*"
      ],
      "@images/*": [
        "src/images/*"
      ],
      "@scripts/*": [
        "src/assets/scripts/*"
      ],
      "@styles/*": [
        "src/assets/styles/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "allowJs": true,
    "types": ["astro/client"]
  }
}
